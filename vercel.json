{
  "version": 2,
  "buildCommand": "npm run build:extension",
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/fountain-spell-assist.zip",
      "headers": {
        "Content-Type": "application/zip",
        "Content-Disposition": "attachment; filename=\"fountain-spell-assist.zip\""
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
